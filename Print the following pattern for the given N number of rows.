import java.util.*;

public class Rectangle {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number:");
        int n = sc.nextInt();
        
        int k=n;
        while(k-->0)
        {
            System.out.println();

            for(int i=0;i<n;i++)
            {
                System.out.print(n);
            }
        }
    }
}
